<li>{{new-grower saveGrower="saveGrower"}}</li>
<h2>Growers:</h2>
<ul>
  {{#each model.growers as |grower|}}
  {{grower-tile grower=grower destroyGrower="destroyGrower"}}
  {{update-grower updateGrower="updateGrower" grower=grower}}
  {{/each}}
</ul>


<h2>Strains Available:</h2>
<ul>
  {{#each model.strains as |strain|}}
  <li>
  <button class="btn btn-success btn-xs" {{action 'addToCart' strain}}><span class="glyphicon glyphicon-plus"></span></button>
  {{#link-to 'strain' strain.id}}{{strain.name}}{{/link-to}} - {{strain.type}} -THC- {{strain.thc}}% -CBD- {{strain.cbd}}% --Price: ${{strain.price}}</li>
  {{/each}}
</ul>

{{display-cart}}
