{{yield}}
<h4>Strains Grown By</h4>
<li>{{grower.name}}</li>
<li>{{grower.location}}</li>

{{#each grower.strains as |strain|}}
{{strain-tile strain=strain}}
<button {{action 'destroyStrain' strain}}>Delete Strain</button>
<hr>
{{/each}}
